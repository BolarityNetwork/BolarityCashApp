{
  "name": "BolarityCashApp",
  "main": "entrypoint.js",
  "version": "1.0.0",
  "engines": {
    "node": "20.19.4",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "expo lint",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "quick-update": "EXPO_USE_CHUNKED_REQUESTS=1 EAS_BUILD_GRAPHQL_TIMEOUT=300000 node scripts/quick-update.js",
    "eas-build-android": "eas build --platform android --local --profile production",
    "eas-build-ios": "eas build --platform ios --local --profile production",
    "build-ios": "react-native bundle --entry-file='entrypoint.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  },
